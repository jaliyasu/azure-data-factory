{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "jsonconnection",
						"type": "DatasetReference"
					},
					"name": "jsonsource2024"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "flatten1"
				},
				{
					"name": "flatten2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          quiz as (maths as (q1 as (answer as short, options as short[], question as string), q2 as (answer as short, options as short[], question as string)), sport as (q1 as (answer as string, options as string[], question as string)))",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'arrayOfDocuments') ~> jsonsource2024",
				"jsonsource2024 foldDown(unroll(quiz.sport.q1.options),",
				"     mapColumn(",
				"          sport_quiz_1 = quiz.maths.q1.question",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1",
				"jsonsource2024 foldDown(unroll(quiz.maths.q1.options),",
				"     mapColumn(",
				"          math_question = quiz.maths.q1.question,",
				"          options = quiz.maths.q1.options",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten2"
			]
		}
	}
}